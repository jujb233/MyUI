{
  "name": "@jujb233/myui",
  "private": false,
  "version": "0.2.0",
  "type": "module",
  "description": "一个使用Taildwind CSS的现代Web UI组件库",
  "keywords": [
    "solid-js",
    "ui",
    "components",
    "tailwind",
    "typescript"
  ],
  "author": "jujb233",
  "license": "MIT",
  "homepage": "https://github.com/jujb233/MyUI#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jujb233/MyUI.git"
  },
  "bugs": {
    "url": "https://github.com/jujb233/MyUI/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./web-components": {
      "types": "./dist/src/web-components.d.ts",
      "import": "./dist/web-components.js",
      "require": "./dist/web-components.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "dev:demos": "vite --config vite.demo.config.ts",
    "build:demos": "vite build --config vite.demo.config.ts",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "clsx": "^2.1.1",
    "solid-element": "^1.9.1",
    "solid-js": "^1.9.10",
    "tailwindcss": "^4.1.17",
    "vite-plugin-solid": "^2.11.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  }
}