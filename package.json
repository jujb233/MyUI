{
  "name": "@jujb233/myui",
  "private": false,
  "version": "19.4.4",
  "type": "module",
  "description": "一个使用Taildwind CSS的现代React UI组件库",
  "keywords": [
    "react",
    "ui",
    "components",
    "tailwind",
    "typescript"
  ],
  "author": "jujb233",
  "license": "MIT",
  "homepage": "https://github.com/jujb233/MyUI#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jujb233/MyUI.git"
  },
  "bugs": {
    "url": "https://github.com/jujb233/MyUI/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "clsx": "^2.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.5"
  }
}
